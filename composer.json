{
    "name": "araise/search-bundle",
    "type": "symfony-bundle",
    "description": "Full text search on Doctrine entities without external dependencies.",
    "license": "MIT",
    "homepage": "https://araise.dev",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "whatwedo GmbH",
            "email": "welove@whatwedo.ch"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "doctrine/annotations": "^1.13.2",
        "doctrine/orm": "^2.11.0",
        "araise/core-bundle": "dev-feature/symfony-7",
        "doctrine/doctrine-migrations-bundle": "^3.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^v6.0|^7.0",
        "symfony/config": "^v6.0|^7.0",
        "symfony/dependency-injection": "^v6.0|^7.0",
        "symfony/yaml": "^v6.0|^7.0",
        "symfony/validator": "^6.0|^7.0",
        "symfony/orm-pack": "^1.0",
        "symfony/translation": "^6.0|^7.0",
        "symfony/twig-bundle": "^6.0|^7.0",
        "nyholm/symfony-bundle-test": "^1.8",
        "doctrine/doctrine-bundle": "^2.5.5",
        "whatwedo/php-coding-standard": "^1.0",
        "zenstruck/foundry": "^1.16",
        "zenstruck/console-test": "^v1.1.0",
        "phpstan/phpstan": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "araise\\SearchBundle\\": "src/",
            "araise\\SearchBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
